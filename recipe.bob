{
    "project": {
        "name": "Bob the Builder",
        "shortName": "bob",
        "version": "0.0.1",
        "dependencies": [
            {
                "dependency": "glib",
                "version": "2.0"
            }, {
                "dependency": "gobject",
                "version": "2.0"
            }, {
                "dependency": "gio",
                "version": "2.0"
            }, {
                "dependency": "json-glib",
                "version": "1.0"
            }, {
                "dependency": "libvala",
                "version": "0.28"
            }, {
                "dependency": "gmodule",
                "version": "2.0"
            }, {
                "dependency": "gee",
                "version": "0.8"
            }
        ]
    },
    "plugins": {
        "collect-sources": {
            "verbose": true
        },
        "uncrustify": {
            "verbose": true,
            "configuration": "uncrustify.cfg",
            "replace": true,
            "backup": false
        },
        "build": {
            "verbose": true
        },
        "repository:install": {
            "overwrite": true
        }
    }
}